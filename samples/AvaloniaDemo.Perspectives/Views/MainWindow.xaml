<Window x:Class="AvaloniaDemo.Views.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:idc="clr-namespace:Dock.Avalonia.Controls;assembly=Dock.Avalonia"
        xmlns:views="clr-namespace:AvaloniaDemo.Views;assembly=AvaloniaDemo"
        mc:Ignorable="d"
        d:DesignWidth="1200" d:DesignHeight="680"
        UseLayoutRounding="True" RenderOptions.BitmapInterpolationMode="HighQuality"
        WindowState="Normal" WindowStartupLocation="CenterScreen"
        Foreground="{DynamicResource ThemeForegroundBrush}"
        BorderThickness="1" BorderBrush="{DynamicResource ThemeBorderLowBrush}"
        Title="Dock Avalonia Demo" Height="680" Width="1200">
    <views:MainView/>
</Window>
