      <SolidColorBrush x:Key="swWidgetTabSelectedBackground">#FFFFFF</SolidColorBrush>
      <StaticResource x:Key="swWidgetTabSelectedForeground" ResourceKey="DockTabForegroundBrush" />
      <StaticResource x:Key="swWidgetTabActiveBackground" ResourceKey="DockTargetIndicatorBrush" />
      <StaticResource x:Key="swForegroundTitle" ResourceKey="DockTabForegroundBrush" />
      <SolidColorBrush x:Key="swColorTextDefaultPointerOverBrush">#0A1A32</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonBackgroundColor">#FFFFFF</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonFillPointerOverBrush">#D8E6FF</SolidColorBrush>
      <SolidColorBrush x:Key="swFeedbackLogsBackground">#E8EEF9</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonDisabledBrush">#8B96AC</SolidColorBrush>
    </ResourceDictionary>

    <ResourceDictionary x:Key="Dark">
      <!-- Token-first brushes -->
      <SolidColorBrush x:Key="DockSurfaceWorkbenchBrush">#1C2230</SolidColorBrush>
      <StaticResource x:Key="DockSurfaceSidebarBrush" ResourceKey="DockSurfaceWorkbenchBrush" />
      <SolidColorBrush x:Key="DockSurfaceEditorBrush">#20283A</SolidColorBrush>
      <StaticResource x:Key="DockSurfacePanelBrush" ResourceKey="DockSurfaceWorkbenchBrush" />
      <SolidColorBrush x:Key="DockSurfaceHeaderBrush">#334055</SolidColorBrush>
      <SolidColorBrush x:Key="DockSurfaceHeaderActiveBrush">#5C9BFF</SolidColorBrush>

      <StaticResource x:Key="DockBorderSubtleBrush" ResourceKey="DockSurfaceHeaderBrush" />
      <SolidColorBrush x:Key="DockBorderStrongBrush">#415271</SolidColorBrush>
      <StaticResource x:Key="DockSeparatorBrush" ResourceKey="DockBorderSubtleBrush" />
      <StaticResource x:Key="DockSplitterIdleBrush" ResourceKey="DockBorderSubtleBrush" />
      <SolidColorBrush x:Key="DockSplitterHoverBrush">#4C89E8</SolidColorBrush>
      <SolidColorBrush x:Key="DockSplitterDragBrush">#5C9BFF</SolidColorBrush>

      <SolidColorBrush x:Key="DockTabBackgroundBrush">#00000000</SolidColorBrush>
      <SolidColorBrush x:Key="DockTabHoverBackgroundBrush">#4C89E8</SolidColorBrush>
      <SolidColorBrush x:Key="DockTabActiveBackgroundBrush">#2C72E3</SolidColorBrush>
      <StaticResource x:Key="DockTabActiveIndicatorBrush" ResourceKey="DockTabActiveBackgroundBrush" />
      <SolidColorBrush x:Key="DockTabForegroundBrush">#E8F1FF</SolidColorBrush>
      <StaticResource x:Key="DockTabSelectedForegroundBrush" ResourceKey="DockTabForegroundBrush" />
      <SolidColorBrush x:Key="DockTabActiveForegroundBrush">#E8F1FF</SolidColorBrush>
      <SolidColorBrush x:Key="DockTabCloseHoverBackgroundBrush">#24354D</SolidColorBrush>

      <SolidColorBrush x:Key="DockTargetIndicatorBrush">#5C9BFF</SolidColorBrush>
      <SolidColorBrush x:Key="DockChromeButtonHoverBackgroundBrush">#2C374E</SolidColorBrush>
      <StaticResource x:Key="DockChromeButtonPressedBackgroundBrush" ResourceKey="DockSurfaceHeaderBrush" />
      <StaticResource x:Key="DockChromeButtonDangerHoverBrush" ResourceKey="DockTabCloseHoverBackgroundBrush" />

      <!-- Legacy key aliases -->
      <StaticResource x:Key="DockApplicationAccentBrushLow" ResourceKey="DockTabActiveBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushMed" ResourceKey="DockTabHoverBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushHigh" ResourceKey="DockTabCloseHoverBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentForegroundBrush" ResourceKey="DockTabActiveForegroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushIndicator" ResourceKey="DockTargetIndicatorBrush" />
      <StaticResource x:Key="DockThemeBackgroundBrush" ResourceKey="DockSurfacePanelBrush" />
      <StaticResource x:Key="DockThemeControlBackgroundBrush" ResourceKey="DockSurfaceEditorBrush" />
