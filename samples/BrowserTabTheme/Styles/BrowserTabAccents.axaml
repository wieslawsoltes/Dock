      <StaticResource x:Key="DockThemeForegroundBrush" ResourceKey="DockTabForegroundBrush" />
      <StaticResource x:Key="DockThemeAccentBrush" ResourceKey="DockSurfaceHeaderActiveBrush" />
      <StaticResource x:Key="DockToolChromeIconBrush" ResourceKey="DockThemeForegroundBrush" />
      <StaticResource x:Key="DockChromeButtonForegroundBrush" ResourceKey="DockToolChromeIconBrush" />

      <SolidColorBrush x:Key="CaptionButtonBackground">#334055</SolidColorBrush>
      <SolidColorBrush x:Key="CaptionButtonBorderBrush">#415271</SolidColorBrush>
      <SolidColorBrush x:Key="CaptionButtonForeground">#E8F1FF</SolidColorBrush>

      <StaticResource x:Key="swWidgetBackground" ResourceKey="DockSurfaceEditorBrush" />
      <SolidColorBrush x:Key="swWidgetTabBackground">#2A3347</SolidColorBrush>
      <StaticResource x:Key="swWidgetTabForeground" ResourceKey="DockTabForegroundBrush" />
      <SolidColorBrush x:Key="swWidgetTabHoverBackground">#35425D</SolidColorBrush>
      <SolidColorBrush x:Key="swWidgetTabSelectedBackground">#20283A</SolidColorBrush>
      <StaticResource x:Key="swWidgetTabSelectedForeground" ResourceKey="DockTabForegroundBrush" />
      <StaticResource x:Key="swWidgetTabActiveBackground" ResourceKey="DockTargetIndicatorBrush" />
      <StaticResource x:Key="swForegroundTitle" ResourceKey="DockTabForegroundBrush" />
      <SolidColorBrush x:Key="swColorTextDefaultPointerOverBrush">#F3F8FF</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonBackgroundColor">#2C374E</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonFillPointerOverBrush">#314464</SolidColorBrush>
      <SolidColorBrush x:Key="swFeedbackLogsBackground">#2C374E</SolidColorBrush>
      <SolidColorBrush x:Key="swButtonDisabledBrush">#7A89A5</SolidColorBrush>
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>

  <sys:Double x:Key="DockFontSizeNormal">12</sys:Double>
  <sys:Double x:Key="DockDragPreviewFontSize">10</sys:Double>

  <!-- Size/density tokens -->
  <sys:Double x:Key="DockTabItemMinHeight">24</sys:Double>
  <sys:Double x:Key="DockToolTabItemMinHeight">0</sys:Double>
  <Thickness x:Key="DockDocumentTabItemPadding">4,0,4,0</Thickness>
  <Thickness x:Key="DockToolTabItemPadding">4,1,4,0</Thickness>
  <Thickness x:Key="DockToolTabItemSelectedPadding">4,2,4,0</Thickness>
  <sys:Double x:Key="DockCreateButtonWidth">26</sys:Double>
  <sys:Double x:Key="DockCreateButtonHeight">24</sys:Double>
  <sys:Double x:Key="DockCloseButtonSize">14</sys:Double>
  <sys:Double x:Key="DockChromeButtonWidth">18</sys:Double>
  <sys:Double x:Key="DockChromeButtonHeight">16</sys:Double>
  <Thickness x:Key="DockChromeButtonPadding">3</Thickness>
  <Thickness x:Key="DockChromeButtonMargin">2,0,0,0</Thickness>
  <sys:Double x:Key="DockChromeGripHeight">5</sys:Double>
  <Thickness x:Key="DockChromeGripMargin">0,0,2,0</Thickness>
  <SolidColorBrush x:Key="DockChromeGripBrush" Color="#00000000" />
  <Thickness x:Key="DockHeaderContentPadding">2</Thickness>
  <Thickness x:Key="DockModifiedIndicatorMargin">0,0,2,0</Thickness>
