      <StaticResource x:Key="DockTabActiveIndicatorBrush" ResourceKey="DockTabActiveBackgroundBrush" />
      <SolidColorBrush x:Key="DockTabForegroundBrush">#0E1D36</SolidColorBrush>
      <StaticResource x:Key="DockTabSelectedForegroundBrush" ResourceKey="DockTabForegroundBrush" />
      <SolidColorBrush x:Key="DockTabActiveForegroundBrush">#0E1D36</SolidColorBrush>
      <SolidColorBrush x:Key="DockTabCloseHoverBackgroundBrush">#D8E6FF</SolidColorBrush>

      <StaticResource x:Key="DockTargetIndicatorBrush" ResourceKey="DockTabActiveBackgroundBrush" />
      <SolidColorBrush x:Key="DockChromeButtonHoverBackgroundBrush">#E8EEF9</SolidColorBrush>
      <StaticResource x:Key="DockChromeButtonPressedBackgroundBrush" ResourceKey="DockSurfaceHeaderBrush" />
      <StaticResource x:Key="DockChromeButtonDangerHoverBrush" ResourceKey="DockTabCloseHoverBackgroundBrush" />

      <!-- Legacy key aliases -->
      <StaticResource x:Key="DockApplicationAccentBrushLow" ResourceKey="DockTabActiveBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushMed" ResourceKey="DockTabHoverBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushHigh" ResourceKey="DockTabCloseHoverBackgroundBrush" />
      <StaticResource x:Key="DockApplicationAccentForegroundBrush" ResourceKey="DockTabActiveForegroundBrush" />
      <StaticResource x:Key="DockApplicationAccentBrushIndicator" ResourceKey="DockTargetIndicatorBrush" />
      <StaticResource x:Key="DockThemeBackgroundBrush" ResourceKey="DockSurfacePanelBrush" />
      <StaticResource x:Key="DockThemeControlBackgroundBrush" ResourceKey="DockSurfaceEditorBrush" />
      <StaticResource x:Key="DockThemeBorderLowBrush" ResourceKey="DockBorderSubtleBrush" />
      <StaticResource x:Key="DockThemeForegroundBrush" ResourceKey="DockTabForegroundBrush" />
      <StaticResource x:Key="DockThemeAccentBrush" ResourceKey="DockSurfaceHeaderActiveBrush" />
      <StaticResource x:Key="DockToolChromeIconBrush" ResourceKey="DockThemeForegroundBrush" />
      <StaticResource x:Key="DockChromeButtonForegroundBrush" ResourceKey="DockToolChromeIconBrush" />

      <SolidColorBrush x:Key="CaptionButtonBackground">#E8EEF9</SolidColorBrush>
      <SolidColorBrush x:Key="CaptionButtonBorderBrush">#CBD9F2</SolidColorBrush>
      <SolidColorBrush x:Key="CaptionButtonForeground">#0E1D36</SolidColorBrush>

      <StaticResource x:Key="swWidgetBackground" ResourceKey="DockSurfaceEditorBrush" />
      <SolidColorBrush x:Key="swWidgetTabBackground">#E5ECF8</SolidColorBrush>
      <StaticResource x:Key="swWidgetTabForeground" ResourceKey="DockTabForegroundBrush" />
