<UserControl x:Class="DockFigmaSample.Views.Documents.CanvasDocumentView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:rxui="clr-namespace:Avalonia.ReactiveUI;assembly=Avalonia.ReactiveUI"
             xmlns:vm="using:DockFigmaSample.ViewModels.Documents"
             x:DataType="vm:CanvasDocumentViewModel" x:CompileBindings="True">
  <Grid>
    <rxui:RoutedViewHost Router="{Binding Router}" />
    <Border HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Margin="16"
            Padding="10,6"
            CornerRadius="10"
            Background="{DynamicResource CanvasOverlayBrush}"
            BorderBrush="{DynamicResource PanelBorderBrush}"
            BorderThickness="1">
      <StackPanel>
        <TextBlock Text="Canvas" Classes="overlay-title" />
        <TextBlock Text="Frame: Landing Page" Classes="muted" />
      </StackPanel>
    </Border>
  </Grid>
</UserControl>
