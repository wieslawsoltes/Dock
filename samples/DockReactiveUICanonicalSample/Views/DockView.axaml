<UserControl x:Class="DockReactiveUICanonicalSample.Views.DockView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="clr-namespace:Dock.Model.Controls;assembly=Dock.Model"
             x:CompileBindings="True"
             x:DataType="DockViewModel">
  <OverlayHost Margin="8"
               DataContext="{Binding Layout}">
    <DockControl Layout="{Binding}" x:DataType="controls:IRootDock" />
  </OverlayHost>
</UserControl>
