<UserControl x:Class="DockReactiveUICanonicalSample.Views.DockView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="clr-namespace:Dock.Model.Controls;assembly=Dock.Model"
             xmlns:vm="using:Dock.Model.ReactiveUI.Navigation.ViewModels"
             x:CompileBindings="True"
             x:DataType="vm:DockViewModel">
  <OverlayHost Margin="8"
               DataContext="{Binding Layout}">
    <DockControl Layout="{Binding}"
                 ControlRecyclingDataTemplate.ControlRecycling="{DynamicResource DockControlRecycling}"
                 x:DataType="controls:IRootDock">
      <DockControl.Resources>
        <ControlRecycling x:Key="DockControlRecycling" TryToUseIdAsKey="True" />
      </DockControl.Resources>
    </DockControl>
  </OverlayHost>
</UserControl>
