<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="using:DockReactiveUIWorkspaceSample.Views"
             xmlns:vm="using:DockReactiveUIWorkspaceSample.ViewModels"
             x:Class="DockReactiveUIWorkspaceSample.App">

  <Application.DataTemplates>
    <DataTemplate DataType="vm:DocumentViewModel">
      <views:DocumentView />
    </DataTemplate>
    <DataTemplate DataType="vm:ToolViewModel">
      <views:ToolView />
    </DataTemplate>
  </Application.DataTemplates>

  <Application.Styles>
    <FluentTheme />
    <DockFluentTheme />
  </Application.Styles>
</Application>
